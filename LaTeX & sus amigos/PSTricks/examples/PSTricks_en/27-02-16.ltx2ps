\psset{fontscale=10}
\makebox[2em]{x(deg)}       \makebox[5em]{$sin x$}
\makebox[5em]{$cos x$}      \makebox[5em]{$\sqrt x$}
\makebox[7em]{$sin x+cos x$}\makebox[6em]{$sin^2 x+cos^2 x$}\\[3pt]
