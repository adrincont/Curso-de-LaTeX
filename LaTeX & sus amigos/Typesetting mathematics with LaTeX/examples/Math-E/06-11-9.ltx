\usepackage{amsmath}  \newcounter{mySubCounter}
\newcommand\twocoleqn[2]{\setcounter{mySubCounter}{0}\let\OldTheEq\theequation%
  \renewcommand\theequation{\OldTheEq\alph{mySubCounter}}%
  \noindent\begin{minipage}{.49\textwidth}
    \begin{align}\refstepcounter{mySubCounter}#1\end{align}
  \end{minipage}\hfill\addtocounter{equation}{-1}%
  \begin{minipage}{.49\textwidth}
    \begin{align}\refstepcounter{mySubCounter}#2\end{align}
  \end{minipage}\let\theequation\OldTheEq}
\StopShownPreambleCommands
